<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="htp://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
		http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">

	<display-name>Magento Webservice</display-name>

	<servlet>
		<servlet-name>RegistrationServletClass</servlet-name>
		<servlet-class>com.sumanth.magneto.RegistrationServletClass</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegistrationServletClass</servlet-name>
		<url-pattern>/submit</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>Register.jsp</welcome-file>
	</welcome-file-list>
</web-app>